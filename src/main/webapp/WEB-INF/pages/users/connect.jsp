<form action="connect" method="POST" class="form-signin">
  <h2 class="form-signin-heading">Connectez-vous</h2>
  <label for="inputEmail" class="sr-only">Nom d'utilisateur</label>
  <input type="email" id="inputEmail" label="username" class="form-control" placeholder="Nom d'utilisateur" required autofocus>
  <label for="inputPassword" class="sr-only">Mot de passe</label>
  <input type="password" id="inputPassword" label ="password" class="form-control" placeholder="Mot de passe" required>
  <button class="btn btn-lg btn-primary btn-block" type="submit">Se connecter</button>
</form>
